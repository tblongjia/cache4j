<?xml version="1.0" encoding="Windows-1251"?>
 

<cache-factory clean-interval="10s">
 
     <cache id="cache_id1" desc="example cache 1"
            ttl="1h"
            idle="30m"
            max-size="10000"
            max-memory-size="50m" />
 
     <cache id="cache_id2" desc="example cache 2"
            ttl="30m"
            idle="20m"
            max-size="1000"
            max-memory-size="30m"
            type="blocking"
            algorithm="lfu"
            reference="soft" />

</cache-factory>
